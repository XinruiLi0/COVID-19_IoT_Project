        <label class="form-check-label lead font-weight-bold" for="exampleCheck1">Please input Device Description: </label>
        <input id="descr" type="text" size="35" placeholder="Device Description here......" name="devid">
        <br>
        <input class="btn btn-primary" id="searching" onClick="RegAjax()" type="button" value="Register">
        <br>
        <br>
        <label class="form-check-label lead font-weight-bold" for="exampleCheck1">Input Device ID to delete: </label>
        <input id="delid" type="text" size="25" placeholder="Device ID here......" name="delid">
        <br>
        <input class="btn btn-primary" id="searching" onClick="delAjax()" type="button" value="Delete">
    </form>

</div>
